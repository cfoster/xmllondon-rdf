@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swc: <http://data.semanticweb.org/ns/swc/ontology#> .
@prefix xlswc: <http://xmllondon.com/ns/swc/ontology#> .

@prefix xmllondon: <http://data.semanticweb.org/conference/xmllondon/> .
@prefix xmlprague: <http://www.xmlprague.cz/> .
@prefix xmlss: <http://xmlsummerschool.com/> .
@prefix xmlamsterdam: <http://xmlamsterdam.com/> .

## 2015 ##

xmlprague:2015 a swc:ConferenceEvent ;
  xlswc:sisterEvent xmllondon:2015 ;
  xlswc:sisterEvent xmlss:2015 ;
  xlswc:relatedEvent xmlprague:2014 ;
  xlswc:relatedEvent xmlprague:2013 ;
  rdfs:label "XML Prague 2015" .

xmlss:2015 a swc:AcademicEvent ;
  xlswc:sisterEvent xmllondon:2015 ;
  xlswc:sisterEvent xmlprague:2015 ;
  xlswc:relatedEvent xmlss:2014 ;
  xlswc:relatedEvent xmlss:2013 ;
  rdfs:label "XML Summer School 2015" .

